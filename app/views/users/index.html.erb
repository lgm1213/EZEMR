<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="table">
  <thead>
    <tr>
      <th>  Username   </th>
      <th>  Last Name  </th>
      <th>  First Name </th>
      <th>  Middle Initial </th>
      <th>  Gender  </th>
      <th>  Date of Birth </th>
      <th>  SSN </th>
      <th>  Address </th>
      <th>  City </th>
      <th>  State </th></td>
      <th>  Zip Code </th>
      <th>  Phone </th>
      <th>  Email </th>
      <th>  Activated At  </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody class= 'table-striped'>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %>  </td>
        <td><%= user.last_name %>  </td>
        <td><%= user.first_name %>  </td>
        <td><%= user.middle_initial %>  </td>
        <td><%= user.gender %>  </td>
        <td><%= user.dob %></td>
        <td><%= user.activated_at %>  </td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
